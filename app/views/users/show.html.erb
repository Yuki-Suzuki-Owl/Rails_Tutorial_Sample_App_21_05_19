<% provide(:title,@user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user,size:100 %>
        <%= @user.name %>
      </h1>
    </section>
  </aside>
</div>

<p><%= @user.name %>,<%= @user.email %></p>
<p><%= @user.created_at %></p>
<p><%= @user.updated_at %></p>
<p><%= Time.now %></p>
